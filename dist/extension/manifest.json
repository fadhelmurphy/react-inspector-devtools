{
  "manifest_version": 3,
  "name": "FDL React DevTools Inspector",
  "version": "0.5",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.bundle.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["injected.bundle.js"],
      "matches": ["<all_urls>"]
    }
  ]
}

